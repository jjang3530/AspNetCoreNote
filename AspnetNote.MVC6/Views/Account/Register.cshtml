@model AspnetNote.MVC6.Models.User

<h2>SIGNIN</h2>

<form class="form-horizontal" asp-controller="Account" asp-action="Register" method="post">
    <div class="form-group">
        <label>User ID</label>
        <input type="text" class="form-control" asp-for="UserId" placeholder="USER ID Enter" />
        <span class="text-danger" asp-validation-for="UserId"></span>
    </div>

    <div class="form-group">
        <label>User PW</label>
        <input type="password" class="form-control" asp-for="UserPassword" placeholder="USER PW Enter" />
        <span class="text-danger" asp-validation-for="UserPassword"></span>
    </div>

    <div class="form-group">
        <label>User Name</label>
        <input type="text" class="form-control" asp-for="UserName" placeholder="USER Name Enter" />
        <span class="text-danger" asp-validation-for="UserName"></span>
    </div>

    <div class="form-group">
        <button type="submit" class="btn btn-primary">SIGN IN</button>
        <a class="btn btn-warning" href="#">CANCEL</a>
    </div>
</form>